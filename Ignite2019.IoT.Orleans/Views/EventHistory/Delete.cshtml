@model Ignite2019.IoT.Orleans.ViewModel.EventHistoryVMs.EventHistoryVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.DeviceId" />
<wt:display field="Entity.EventType" />
<wt:display field="Entity.Detail" />
<wt:display field="Entity.UpdateTime" />
<wt:display field="Entity.UserId" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>