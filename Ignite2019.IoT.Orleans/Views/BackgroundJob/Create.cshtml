@model Ignite2019.IoT.Orleans.ViewModel.BackgroundJobVMs.BackgroundJobVM

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.DeviceId" />
<wt:combobox field="Entity.JobStatus" />
<wt:textbox field="Entity.Command" />
<wt:datetime type="DateTime" field="Entity.StartTime" />
<wt:textbox field="Entity.Period" />
<wt:datetime  type="DateTime" field="Entity.EndTime" />
</wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>