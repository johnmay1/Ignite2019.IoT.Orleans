@model Ignite2019.IoT.Orleans.ViewModel.ProductVMs.ProductVM

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.Name" />
<wt:textbox field="Entity.Description" />
<wt:textbox field="Entity.Version" />
<wt:combobox field="Entity.ProductType" />
<wt:combobox field="Entity.ProtocolType" />
<wt:combobox field="Entity.CompanyId" items="AllCompanys"/>
</wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>